<div class="container-lg" id="update-course">
<%= simple_form_for(course) do |f| %>
  <%= f.input :name %>
  <%= f.input :description %>
  <%= f.input :photo, as: :file %>
  <%= f.input :duration,  input_html: { type: 'number' } %>
  <%= f.input :price,  input_html: { type: 'number' } %>
  <%= f.submit class: "btn btn-outline-success"%>
<% end %>
</div>
