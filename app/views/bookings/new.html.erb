<% if !current_user.teacher %>
  <%= simple_form_for [ @course, @booking ] do |f| %>
    <%= f.association :occurence, prompt: "Select time", label_method: :slot, value_method: :id  %>
    <%= f.submit %>
  <% end %>
<% end %>

